{
  "$schema": "http://cachemanager.net/schemas/cachemanager.json",
  "redis": [
    {
      "key": "RedisA",
      "endpoints": [
        {
          "host": "127.0.0.1",
          "port": 6379
        }
      ],
      "connectionTimeout": 5000,
      "allowAdmin": true,
      "database": 0
    },
    {
      "key": "RedisB",
      "endpoints": [
        {
          "host": "127.0.0.1",
          "port": 6379
        }
      ],
      "connectionTimeout": 4000,
      "allowAdmin": true,
      "database": 0
    }
  ],
  "cacheManagers": [
    {
      "name": "CacheA",
      "backplane": {
        "key": "RedisA",
        "knownType": "Redis"
      },
      "serializer": { "knownType": "Json" },
      "handles": [
        {
          "knownType": "Dictionary",
          "expirationMode": "Absolute",
          "expirationTimeout": "00:00:10"
        },
        {
          "knownType": "Redis",
          "name": "RedisA",
          "key": "RedisA",
          "isBackplaneSource": true
        }
      ]
    },
    {
      "name": "CacheB",
      "backplane": {
        "key": "RedisB",
        "knownType": "Redis"
      },
      "handles": [
        {
          "knownType": "Dictionary",
          "expirationMode": "Absolute",
          "expirationTimeout": "00:00:10"
        },
        {
          "knownType": "Redis",
          "name": "RedisB",
          "key": "RedisB",
          "isBackplaneSource": true
        }
      ]
    }
  ]
}
