{
  "$schema": "http://cachemanager.net/schemas/cachemanager.json#",
  "redis": [
    {
      "key": "redisConnection",
      "endpoints": [
        {
          "host": "localhost",
          "port": 6379
        }
      ],
      "allowAdmin": true,
      "database": 11,
      "isSsl": true,
      "password": "password",
      "sslHost": "sslHost",
      "connectionTimeout": 123
    },
    {
      "key": "redisConString",
      "connectionString": "localhost:6379,allowAdmin=true"
    }
  ],
  "cacheManagers": [
    {
      "maxRetries": 1000,
      "name": "cachename",
      "retryTimeout": 100,
      "updateMode": "Full",
      "backplane": {
        "key": "redisConString",
        "knownType": "Redis",
        "channelName": "test"
      },
      "loggerFactory": {
        "knownType": "Microsoft"
      },
      "serializer": {
        "knownType": "Json"
      },
      "handles": [
        {
          "knownType": "Dictionary",
          "enablePerformanceCounters": true,
          "enableStatistics": true,
          "expirationMode": "Absolute",
          "expirationTimeout": "0:0:23",
          "isBackplaneSource": false,
          "name": "sys cache"
        },
        {
          "knownType": "Redis",
          "key": "redisConString",
          "isBackplaneSource": true
        }
      ]
    }
  ]
}
